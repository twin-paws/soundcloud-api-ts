{
  "name": "soundcloud-api-ts",
  "version": "1.6.5",
  "description": "A TypeScript client for the SoundCloud API. Zero dependencies, native fetch, full OAuth 2.1 + PKCE support.",
  "bin": {
    "sc-cli": "./dist/cli.js",
    "soundcloud-cli": "./dist/cli.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "docs": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/twin-paws/soundcloud-api-ts.git"
  },
  "keywords": [
    "soundcloud",
    "api",
    "typescript",
    "oauth2",
    "music",
    "streaming",
    "audio",
    "sdk",
    "client",
    "rest-api",
    "soundcloud-api",
    "pkce",
    "node"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "tsup": "^8.0.0",
    "typedoc": "^0.28.16",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "homepage": "https://twin-paws.github.io/soundcloud-api-ts/"
}
